<nav aria-label="Breadcrumb" class="breadcrumbs">
  <ul class="inline-list breadcrumb-list" style="--list-separator: ' Â» '">
    {% for crumb in breadcrumb %}
      <li>
        <a
          href="{{crumb.url}}"
          {% if loop.last %} aria-current="page" {% endif %}
        >
          {{crumb.name}}
        </a>
      </li>
    {% endfor %}
  </ul>
</nav>